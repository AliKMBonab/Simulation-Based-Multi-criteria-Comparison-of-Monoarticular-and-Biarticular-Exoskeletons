@article{1,
	author = {Rodman, Peter S. and McHenry, Henry M.},
	title = {Bioenergetics and the origin of hominid bipedalism},
	journal = {American Journal of Physical Anthropology},
	volume = {52},
	number = {1},
	pages = {103-106},
	keywords = {Bipedalism, Energetics, Miocene hominoid, Pliocene hominid},
	doi = {10.1002/ajpa.1330520113},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ajpa.1330520113},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajpa.1330520113},
	abstract = {Abstract Compared to most quadrupedal mammals, humans are energetically inefficient when running at high speeds. This fact can be taken to mean that human bipedalism evolved for reasons other than to reduce relative energy cost during locomotion. Recalculation of the energy expended during human walking at normal speeds shows that (1) human bipedalism is at least as efficient as typical mammalian quadrupedalism and (2) human gait is much more efficient than bipedal or quadrupedal locomotion in the chimpanzee. We conclude that bipedalism bestowed an energetic advantage on the Miocene hominoid ancestors of the Hominidae.},
	year = {1980},
	doi = {10.1002/ajpa.1330520113}
}

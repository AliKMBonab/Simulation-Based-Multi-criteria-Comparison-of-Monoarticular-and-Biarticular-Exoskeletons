@article{63,
	author = {Dennis Landin and Melissa Thompson and Meghan Reid},
	title = {Actions of Two Bi-Articular Muscles of the Lower Extremity: A Review},
	journal = {Journal of Clinical Medicine Research},
	volume = {8},
	number = {7},
	year = {2016},
	keywords = {Bi-articular muscles; Lower extremity; Joint angles},
	abstract = {The extremities of the human body contain several bi-articular muscles. The actions produced by muscles at the joints they cross are greatly influenced by joint moment arms and muscle length. These factors are dynamic and subject to change as joint angles are altered. Therefore, to more completely understand the actions of such muscles, the angles of both joints must be manipulated. This report reviews investigations, which have explored the actions of two bi-articular muscles of the lower extremities (gastrocnemius and rectus femoris) as the joints they cross are moved into various combinations of angles. The findings have both clinical and physical performance ramifications.J Clin Med Res. 2016;8(7):489-494doi: http://dx.doi.org/10.14740/jocmr2478w},
	issn = {1918-3011},	url = {https://www.jocmr.org/index.php/JOCMR/article/view/2478}
}

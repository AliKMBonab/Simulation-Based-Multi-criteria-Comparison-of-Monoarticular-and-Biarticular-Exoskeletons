@article{116,
title = "Invariant ankle moment patterns when walking with and without a robotic ankle exoskeleton",
journal = "Journal of Biomechanics",
volume = "43",
number = "2",
pages = "203 - 209",
year = "2010",
issn = "0021-9290",
doi = "https://doi.org/10.1016/j.jbiomech.2009.09.030",
url = "http://www.sciencedirect.com/science/article/pii/S0021929009005491",
author = "Pei-Chun Kao and Cara L. Lewis and Daniel P. Ferris",
keywords = "Gait, Powered orthosis, Locomotion, Inverse dynamics, Joint kinetics, EMG",
abstract = "To guide development of robotic lower limb exoskeletons, it is necessary to understand how humans adapt to powered assistance. The purposes of this study were to quantify joint moments while healthy subjects adapted to a robotic ankle exoskeleton and to determine if the period of motor adaptation is dependent on the magnitude of robotic assistance. The pneumatically powered ankle exoskeleton provided plantar flexor torque controlled by the wearer's soleus electromyography (EMG). Eleven naïve individuals completed two 30-min sessions walking on a split-belt instrumented treadmill at 1.25m/s while wearing the ankle exoskeleton. After two sessions of practice, subjects reduced their soleus EMG activation by ∼36% and walked with total ankle moment patterns similar to their unassisted gait (r2=0.98±0.02, THSD, p>0.05). They had substantially different ankle kinematic patterns compared to their unassisted gait (r2=0.79±0.12, THSD, p<0.05). Not all of the subjects reached a steady-state gait pattern within the two sessions, in contrast to a previous study using a weaker robotic ankle exoskeleton (Gordon and Ferris, 2007). Our results strongly suggest that humans aim for similar joint moment patterns when walking with robotic assistance rather than similar kinematic patterns. In addition, greater robotic assistance provided during initial use results in a longer adaptation process than lesser robotic assistance."
}

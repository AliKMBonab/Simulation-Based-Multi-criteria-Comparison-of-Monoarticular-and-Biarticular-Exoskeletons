clc;clear
path = 'F:\HMI\Exoskeleton\OpenSim\Walking_Mass_Inertia_Effect\Data\Data\Ideal\';
hip_torque = nanmean(csvread(strcat(path,'biarticular_ideal_loaded_hipactuator_torque.csv')),2);
left_torque = circshift(hip_torque,600)
hip_speed = nanmean(csvread(strcat(path,'biarticular_ideal_loaded_hipactuator_speed.csv')),2);
knee_torque = -nanmean(csvread(strcat(path,'biarticular_ideal_loaded_kneeactuator_torque.csv')),2);
knee_speed = nanmean(csvread(strcat(path,'biarticular_ideal_loaded_kneeactuator_speed.csv')),2);
gait_cycle = linspace(0,100,1000);
figure()
subplot(3,2,1)
plot(gait_cycle,hip_torque,'r','LineWidth',2)
title('hip torque')
box off
grid off
subplot(3,2,2)
plot(gait_cycle,knee_torque,'b','LineWidth',2)
title('knee torque')
box off
grid off
subplot(3,2,3)
plot(gait_cycle,hip_speed,'r','LineWidth',2)
title('hip speed')
box off
grid off
subplot(3,2,4)
plot(gait_cycle,knee_speed,'b','LineWidth',2)
title('knee speed')
box off
grid off
subplot(3,2,5)
area(gait_cycle,hip_torque.*hip_speed)
title('hip power')
box off
grid off
subplot(3,2,6)
area(gait_cycle,knee_torque.*knee_speed)
title('knee power')
box off
grid off
